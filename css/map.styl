.single-venue-marker
  border: 1px solid rgb(185, 185, 185)
  padding: 1px
  background: #fff
  border-radius: 2px
  -moz-border-radius: 2px
  -webkit-border-radius: 2px
  .venue
    margin: 1px
    background: rgb(216, 216, 216)
    border: 1px solid rgb(185, 185, 185)
    height: 24px
    width: 24px
    float: left

.cluster-venue-marker
  @extend .single-venue-marker
