.leaflet-div-icon
  background: none
  border: 0

.venue-marker
  background: #fff
  border: 1px solid rgb(185, 185, 185)
  height: 100%
  width: 100%
  padding: 1px
  &, .venue, .inner
    box-sizing: border-box
    border-radius: 2px
    -moz-border-radius: 2px
    -webkit-border-radius: 2px
  .venue
    padding: 1px
    .inner
      background: rgb(216, 216, 216)
      border: 1px solid rgb(185, 185, 185)
      height: 100%
      width: 100%

.single-venue-marker
  .venue
    height: 100%
    width: 100%

.cluster-venue-marker
  .venue
    width: 28px
    height: 28px
    float: left
